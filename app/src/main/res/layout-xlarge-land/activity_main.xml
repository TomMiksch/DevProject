<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent"
                                                   android:id="@+id/constraintLayout">

    <ImageView
            android:layout_width="0dp"
            android:layout_height="150dp" app:srcCompat="@drawable/devslopesbanner"
            android:id="@+id/imageView10"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" android:background="#F2000000"
            app:layout_constraintHorizontal_bias="0.0"/>
    <ImageView
            android:layout_width="150dp"
            android:layout_height="150dp" app:srcCompat="@drawable/devslopesprofilelogo"
            android:id="@+id/logo" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="108dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="32dp" android:layout_marginStart="32dp"/>
    <TextView
            android:text="DevSlopes"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView3" android:layout_marginTop="32dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView10" app:layout_constraintStart_toEndOf="@+id/logo"
            android:textSize="30sp" android:layout_marginLeft="32dp" android:layout_marginStart="32dp"/>
    <ImageView
            android:layout_width="50dp"
            android:layout_height="50dp" app:srcCompat="@drawable/camerabutton"
            android:id="@+id/imageView12"
            app:layout_constraintStart_toEndOf="@+id/textView3" app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintEnd_toStartOf="@+id/imageView20" app:layout_constraintTop_toTopOf="@+id/imageView20"
            app:layout_constraintBottom_toBottomOf="@+id/imageView20"
            app:layout_constraintHorizontal_chainStyle="packed"/>
    <ImageView
            android:layout_width="50dp"
            android:layout_height="50dp" app:srcCompat="@drawable/favoritebutton"
            android:id="@+id/imageView20"
            app:layout_constraintStart_toEndOf="@+id/imageView12" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toStartOf="@+id/imageView21" app:layout_constraintTop_toTopOf="@+id/imageView21"
            app:layout_constraintBottom_toBottomOf="@+id/imageView21" android:layout_marginLeft="32dp"
            android:layout_marginStart="32dp" android:layout_marginRight="32dp" android:layout_marginEnd="32dp"/>
    <ImageView
            android:layout_width="50dp"
            android:layout_height="50dp" app:srcCompat="@drawable/inboxbutton"
            android:id="@+id/imageView21"
            app:layout_constraintStart_toEndOf="@+id/imageView20" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toStartOf="@+id/button2" app:layout_constraintTop_toBottomOf="@+id/imageView10"
            android:layout_marginTop="32dp" android:layout_marginRight="32dp" android:layout_marginEnd="32dp"/>
    <Button
            android:text="CUSTOMIZE DASHBOARD"
            android:layout_width="250dp"
            android:layout_height="wrap_content"
            android:id="@+id/button2" android:textSize="18sp"
            android:textColor="#fff" style="@style/Widget.AppCompat.Button.Borderless"
            app:layout_constraintTop_toBottomOf="@+id/imageView10" android:layout_marginTop="32dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginRight="32dp"
            android:layout_marginEnd="32dp" android:background="@drawable/rounded_gradient"/>
    <androidx.constraintlayout.widget.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                                android:id="@+id/guideline2"
                                                android:orientation="vertical"
                                                app:layout_constraintGuide_percent="0.5"/>
    <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp" app:srcCompat="@drawable/dataimage"
            android:id="@+id/imageView23"
            app:layout_constraintStart_toStartOf="@+id/guideline2" android:layout_marginLeft="16dp"
            android:layout_marginStart="16dp" app:layout_constraintTop_toTopOf="@+id/imageView11"
            app:layout_constraintDimensionRatio="16:9" android:scaleType="centerCrop"/>
    <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp" app:srcCompat="@drawable/videosimage"
            android:id="@+id/imageView25"
            app:layout_constraintStart_toStartOf="@+id/guideline2"
            app:layout_constraintTop_toBottomOf="@+id/imageView23" app:layout_constraintEnd_toEndOf="@+id/imageView23"
            android:layout_marginTop="32dp"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginLeft="16dp" android:layout_marginStart="16dp" android:layout_marginBottom="8dp"
            app:layout_constraintDimensionRatio="h,16:9"/>
    <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp" app:srcCompat="@drawable/codeimage"
            android:id="@+id/imageView11"
            app:layout_constraintEnd_toStartOf="@+id/guideline2" app:layout_constraintTop_toBottomOf="@+id/logo"
            android:layout_marginTop="32dp" android:layout_marginRight="16dp" android:layout_marginEnd="16dp"
            app:layout_constraintDimensionRatio="16:9" android:scaleType="centerCrop"/>
    <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp" app:srcCompat="@drawable/imagesimage"
            android:id="@+id/imageView22"
            android:layout_marginTop="32dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView11"
            app:layout_constraintEnd_toStartOf="@+id/guideline2" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="@+id/imageView11" app:layout_constraintDimensionRatio="16:9"
            android:layout_marginRight="16dp" android:layout_marginEnd="16dp" android:layout_marginBottom="8dp"
            android:scaleType="centerCrop"/>
</androidx.constraintlayout.widget.ConstraintLayout>