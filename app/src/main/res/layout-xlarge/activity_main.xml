<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent">

    <ImageView
            android:layout_width="0dp"
            android:layout_height="250dp" app:srcCompat="@drawable/devslopesbanner"
            android:id="@+id/imageView8"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent" android:scaleType="centerCrop"/>
    <ImageView
            android:layout_width="200dp"
            android:layout_height="200dp" app:srcCompat="@drawable/devslopesprofilelogo"
            android:id="@+id/logo"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginLeft="32dp" android:layout_marginTop="180dp" android:layout_marginStart="32dp"/>
    <TextView
            android:text="DevSlopes"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView2" android:textSize="32sp" app:layout_constraintStart_toEndOf="@+id/logo"
            android:layout_marginTop="44dp" app:layout_constraintTop_toBottomOf="@+id/imageView8"
            android:layout_marginLeft="40dp" android:layout_marginStart="40dp"/>
    <ImageView
            android:layout_width="50dp"
            android:layout_height="50dp" app:srcCompat="@drawable/camerabutton"
            android:id="@+id/imageView13"
            app:layout_constraintStart_toEndOf="@+id/textView2" app:layout_constraintHorizontal_bias="0.76"
            app:layout_constraintEnd_toStartOf="@+id/imageView14" app:layout_constraintTop_toTopOf="@+id/imageView14"
            app:layout_constraintBottom_toBottomOf="@+id/imageView14"
            app:layout_constraintHorizontal_chainStyle="packed"/>
    <ImageView
            android:layout_width="50dp"
            android:layout_height="50dp" app:srcCompat="@drawable/favoritebutton"
            android:id="@+id/imageView14"
            app:layout_constraintStart_toEndOf="@+id/imageView13" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toStartOf="@+id/imageView15" app:layout_constraintTop_toTopOf="@+id/imageView15"
            app:layout_constraintBottom_toBottomOf="@+id/imageView15" android:layout_marginLeft="24dp"
            android:layout_marginRight="24dp"/>
    <ImageView
            android:layout_width="50dp"
            android:layout_height="50dp" app:srcCompat="@drawable/inboxbutton"
            android:id="@+id/imageView15"
            app:layout_constraintStart_toEndOf="@+id/imageView14" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toBottomOf="@+id/imageView8"
            android:layout_marginTop="44dp"/>
    <androidx.constraintlayout.widget.Barrier android:layout_width="wrap_content" android:layout_height="wrap_content"
                                              android:id="@+id/barrier" app:barrierDirection="left"/>
    <androidx.constraintlayout.widget.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                                android:id="@+id/guideline3"
                                                android:orientation="vertical"
                                                app:layout_constraintGuide_percent="0.5"/>
    <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp" app:srcCompat="@drawable/codeimage"
            android:id="@+id/imageView16"
            app:layout_constraintTop_toBottomOf="@+id/logo" android:layout_marginTop="44dp"
            app:layout_constraintEnd_toStartOf="@+id/guideline3" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintDimensionRatio="w,9:16" android:scaleType="centerCrop"
            android:layout_marginRight="16dp" android:layout_marginEnd="16dp" android:layout_marginLeft="32dp"
            android:layout_marginStart="32dp" app:layout_constraintHorizontal_bias="0.0"/>
    <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp" app:srcCompat="@drawable/dataimage"
            android:id="@+id/imageView17"
            android:scaleType="centerCrop"
            app:layout_constraintTop_toTopOf="@+id/imageView16"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginRight="32dp" android:layout_marginEnd="32dp"
            app:layout_constraintDimensionRatio="w,9:16" app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="@+id/guideline3" android:layout_marginLeft="16dp"
            android:layout_marginStart="16dp"/>
    <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp" app:srcCompat="@drawable/imagesimage"
            android:id="@+id/imageView18"
            android:scaleType="centerCrop"
            android:contentDescription="TODO"
            app:layout_constraintTop_toBottomOf="@+id/imageView16"
            android:layout_marginTop="8dp" app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp"/>
    <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp" app:srcCompat="@drawable/videosimage"
            android:id="@+id/imageView19"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toBottomOf="@+id/imageView17"
            android:scaleType="centerCrop" android:layout_marginTop="32dp"
            app:layout_constraintDimensionRatio="w,9:16"
            android:layout_marginRight="32dp"
            android:layout_marginEnd="32dp" app:layout_constraintStart_toStartOf="@+id/guideline3"
            app:layout_constraintHorizontal_bias="1.0" android:layout_marginLeft="16dp"
            android:layout_marginStart="16dp"/>
    <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp" app:srcCompat="@drawable/imagesimage"
            android:id="@+id/imageView9"
            android:scaleType="centerCrop" app:layout_constraintTop_toBottomOf="@+id/imageView16"
            app:layout_constraintEnd_toStartOf="@+id/guideline3" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintDimensionRatio="w,9:16" android:layout_marginTop="32dp"
            android:layout_marginRight="16dp"
            android:layout_marginEnd="16dp" android:layout_marginLeft="32dp" android:layout_marginStart="32dp"
    />
    <Button
            android:text="CUSTOMIZE DASHBOARD"
            android:layout_width="250dp"
            android:layout_height="wrap_content"
            android:id="@+id/button" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.5" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView9" android:layout_marginTop="32dp"
            style="@style/Widget.AppCompat.Button.Borderless" android:background="#C6512F"
            android:textColor="#FFFFFF"/>
</androidx.constraintlayout.widget.ConstraintLayout>
