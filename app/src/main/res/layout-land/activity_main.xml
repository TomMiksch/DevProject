<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent"
                                                   android:id="@+id/constraintLayout">

    <ImageView
            android:layout_width="0dp"
            android:layout_height="125dp" app:srcCompat="@drawable/devslopesbanner"
            android:id="@+id/imageView24"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" android:background="@drawable/rounded_gradient"
            app:layout_constraintHorizontal_bias="0.0" android:scaleType="centerCrop"/>
    <ImageView
            android:layout_width="120dp"
            android:layout_height="109dp" app:srcCompat="@drawable/devslopesprofilelogo"
            android:id="@+id/logo" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="32dp"
            android:layout_marginTop="100dp" android:layout_marginStart="32dp"/>
    <TextView
            android:text="DevSlopes"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView4" android:textSize="18sp"
            app:layout_constraintTop_toBottomOf="@+id/imageView24" app:layout_constraintStart_toEndOf="@+id/logo"
            android:layout_marginTop="32dp" android:layout_marginLeft="32dp" android:layout_marginStart="32dp"/>
    <ImageView
            android:layout_width="50dp"
            android:layout_height="50dp" app:srcCompat="@drawable/camerabutton"
            android:id="@+id/imageView27"
            app:layout_constraintStart_toEndOf="@+id/textView4" app:layout_constraintHorizontal_bias="0.75"
            app:layout_constraintEnd_toStartOf="@+id/imageView28" app:layout_constraintTop_toTopOf="@+id/imageView28"
            app:layout_constraintBottom_toBottomOf="@+id/imageView28"
            app:layout_constraintHorizontal_chainStyle="packed"/>
    <ImageView
            android:layout_width="50dp"
            android:layout_height="50dp" app:srcCompat="@drawable/favoritebutton"
            android:id="@+id/imageView28"
            app:layout_constraintStart_toEndOf="@+id/imageView27" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toStartOf="@+id/imageView29" app:layout_constraintTop_toTopOf="@+id/imageView29"
            app:layout_constraintBottom_toBottomOf="@+id/imageView29" android:layout_marginLeft="32dp"
            android:layout_marginStart="32dp" android:layout_marginRight="32dp" android:layout_marginEnd="32dp"/>
    <ImageView
            android:layout_width="50dp"
            android:layout_height="50dp" app:srcCompat="@drawable/inboxbutton"
            android:id="@+id/imageView29"
            app:layout_constraintStart_toEndOf="@+id/imageView28" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginTop="32dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView24"/>
    <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp" app:srcCompat="@drawable/codeimage"
            android:id="@+id/imageView30"
            app:layout_constraintTop_toTopOf="@+id/imageView31"
            app:layout_constraintBottom_toBottomOf="@+id/imageView31" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.5" app:layout_constraintEnd_toStartOf="@+id/imageView31"
            app:layout_constraintDimensionRatio="1:1" android:scaleType="centerCrop" android:layout_marginLeft="32dp"
            android:layout_marginStart="32dp"/>
    <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp" app:srcCompat="@drawable/dataimage"
            android:id="@+id/imageView31"
            app:layout_constraintTop_toTopOf="@+id/imageView32"
            app:layout_constraintBottom_toBottomOf="@+id/imageView32"
            app:layout_constraintStart_toEndOf="@+id/imageView30" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toStartOf="@+id/imageView32" android:layout_marginLeft="16dp"
            android:layout_marginStart="16dp" android:layout_marginRight="8dp" android:layout_marginEnd="8dp"
            app:layout_constraintDimensionRatio="1:1" android:scaleType="centerCrop"/>
    <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp" app:srcCompat="@drawable/imagesimage"
            android:id="@+id/imageView32"
            app:layout_constraintTop_toTopOf="@+id/imageView33"
            app:layout_constraintBottom_toBottomOf="@+id/imageView33"
            app:layout_constraintStart_toEndOf="@+id/imageView31" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toStartOf="@+id/imageView33" android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp" android:layout_marginRight="16dp" android:layout_marginEnd="16dp"
            android:scaleType="centerCrop" app:layout_constraintDimensionRatio="1:1"/>
    <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp" app:srcCompat="@drawable/videosimage"
            android:id="@+id/imageView33"
            app:layout_constraintStart_toEndOf="@+id/imageView32" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toBottomOf="@+id/imageView28"
            android:layout_marginTop="32dp" app:layout_constraintDimensionRatio="1:1" android:scaleType="centerCrop"
            android:layout_marginRight="32dp" android:layout_marginEnd="32dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="32dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>